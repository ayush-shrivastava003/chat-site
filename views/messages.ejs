<% messages.map(msg => { %>
        <div class="message">
            <div class="author-date">
                <% if (isdev) { %>
                    <p onclick="navigator.clipboard.writeText('<%= msg.author %>')"><%= msg.usrname %></p>
                <% } else { %>
                    <p><%= msg.usrname %></p>
                <% } %>
                <p class="date"><%= msg.date %></p>
            </div>
            <p class="content"><%= msg.content %></p>
        </div>
<% }) %>